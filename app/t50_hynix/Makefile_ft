all: $(TARGET)

clean:
	rm -f $(OBJ)/*.o 
	rm -f *.x 
	rm -f *.flat
	rm -f *.map
	rm -f temp
	rm -f *.img
	rm -f $(TARGET)	
	rm -f *.gdb
	rm -f *.bak

$(TARGET): $(OBJS)
	$(CC) $(LDFLAGS) $(OBJS) /home/secu/freelib/zlib-1.2.11/inflate.o -o $(TARGET) $(LIBS)
	$(STRIP) $(TARGET) 
	cp $(TARGET) /home/secu/tftpboot

